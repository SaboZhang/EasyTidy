<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:common="using:EasyTidy.Common"
                    xmlns:converter="using:CommunityToolkit.WinUI.Converters" 
                    xmlns:cover="using:EasyTidy.Converters">

    <cover:EnumDisplayNameConverter x:Key="EnumDisplayNameConverter"/>
    <cover:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter" />
    <cover:BoolToStringConverter x:Key="BoolToStringConverter" />
    <cover:StringToBoolConverter x:Key="StringToBoolConverter" />
    <cover:LineDashStrokeArrayConverter x:Key="LineDashStrokeArrayConverter" />
    <cover:BoolToEnumConverter x:Key="BoolToEnumConverter"/>
    <cover:EnumToImageConverter x:Key="EnumToImageConverter"/>
    <cover:ValidationEnabledConverter x:Name="CanValidateConverter" />

    <converter:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />

</ResourceDictionary>
